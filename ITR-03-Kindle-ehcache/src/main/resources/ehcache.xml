<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="false" monitoring="off"
	dynamicConfig="false">
	<defaultCache maxEntriesLocalHeap="0" eternal="false" timeToIdleSeconds="1200" timeToLiveSeconds="1200" />
	<cache name="objectCache" maxEntriesLocalHeap="100" eternal="true" memoryStoreEvictionPolicy="FIFO" />
	<cache name="user" maxEntriesLocalHeap="100" eternal="true" memoryStoreEvictionPolicy="FIFO" />
</ehcache>